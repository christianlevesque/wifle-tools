<ResourceDictionary
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush Color="#f32431" x:Key="Primary"/>
	<SolidColorBrush Color="#9e0303" x:Key="PrimaryDark"/>
	<SolidColorBrush Color="White" x:Key="White"/>
	<SolidColorBrush Color="Black" x:Key="Black"/>

	

	<StaticResource x:Key="ButtonBackgroundPointerOver" ResourceKey="Primary"/>
	<StaticResource x:Key="ButtonForegroundPointerOver" ResourceKey="White"/>

	<FontFamily x:Key="ContentControlThemeFontFamily">/Assets/Fonts#Exo 2</FontFamily>
	<x:Double x:Key="ControlContentThemeFontSize">20</x:Double>

	<x:Double x:Key="H1FontSize">40</x:Double>
	<x:Double x:Key="H2FontSize">30</x:Double>
</ResourceDictionary>
