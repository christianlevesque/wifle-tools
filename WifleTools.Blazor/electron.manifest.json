{
	"executable": "WifleTools.Blazor",
	"splashscreen": {
		"imageFile": "/wwwroot/splash-screen.png"
	},
	"name": "WifleTools",
	"author": "Hubber <3",
	"singleInstance": false,
	"environment": "Production",
	"build": {
		"appId": "com.WifleTools.Blazor.app",
		"productName": "WifleTools",
		"copyright": "Copyright Â© 2023",
		"buildVersion": "1.0.0",
		"compression": "maximum",
		"directories": {
			"output": "../../../bin/Desktop"
		},
		"extraResources": [
			{
				"from": "./bin",
				"to": "bin",
				"filter": [
					"**/*"
				]
			}
		],
		"files": [
			{
				"from": "./ElectronHostHook/node_modules",
				"to": "ElectronHostHook/node_modules",
				"filter": [
					"**/*"
				]
			},
			"**/*"
		]
	}
}